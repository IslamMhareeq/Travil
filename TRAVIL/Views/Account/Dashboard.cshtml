@{
    ViewData["Title"] = "Dashboard";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Dashboard - TRAVIL</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <style>
        :root {
            --color-bg: #0a0a0f;
            --color-surface: #12121a;
            --color-surface-2: #1a1a24;
            --color-border: #2a2a3a;
            --color-gold: #d4a853;
            --color-gold-hover: #e4b863;
            --color-white: #ffffff;
            --color-silver: #a0a0b0;
            --color-success: #3fb950;
            --color-warning: #d29922;
            --color-danger: #f85149;
            --color-info: #58a6ff;
            --font-main: 'Plus Jakarta Sans', sans-serif;
            --radius-sm: 6px;
            --radius-md: 10px;
            --radius-lg: 16px;
            --space-1: 4px;
            --space-2: 8px;
            --space-3: 12px;
            --space-4: 16px;
            --space-5: 24px;
            --space-6: 32px;
            --sidebar-width: 260px;
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: var(--font-main);
            background: var(--color-bg);
            color: var(--color-white);
            min-height: 100vh;
        }

        .dashboard-layout {
            display: flex;
            min-height: 100vh;
        }

        /* Sidebar */
        .sidebar {
            width: var(--sidebar-width);
            background: var(--color-surface);
            border-right: 1px solid var(--color-border);
            position: fixed;
            top: 0;
            left: 0;
            height: 100vh;
            display: flex;
            flex-direction: column;
            z-index: 100;
            transition: transform 0.3s ease;
        }

        .sidebar-header {
            padding: var(--space-5);
            border-bottom: 1px solid var(--color-border);
        }

        .sidebar-logo {
            font-size: 1.5rem;
            font-weight: 700;
            color: var(--color-gold);
            text-decoration: none;
            display: flex;
            align-items: center;
            gap: var(--space-2);
        }

        .sidebar-nav {
            flex: 1;
            padding: var(--space-4);
            display: flex;
            flex-direction: column;
            gap: var(--space-2);
        }

        .nav-item {
            display: flex;
            align-items: center;
            gap: var(--space-3);
            padding: var(--space-3) var(--space-4);
            color: var(--color-silver);
            text-decoration: none;
            border-radius: var(--radius-md);
            transition: all 0.2s ease;
        }

            .nav-item:hover, .nav-item.active {
                background: var(--color-surface-2);
                color: var(--color-white);
            }

            .nav-item.active {
                background: linear-gradient(135deg, rgba(212, 168, 83, 0.2), rgba(212, 168, 83, 0.1));
                color: var(--color-gold);
            }

            .nav-item i {
                width: 20px;
                text-align: center;
            }

            .nav-item .badge {
                margin-left: auto;
                background: var(--color-danger);
                color: white;
                padding: 2px 8px;
                border-radius: 10px;
                font-size: 0.75rem;
            }

        .sidebar-footer {
            padding: var(--space-4);
            border-top: 1px solid var(--color-border);
        }

        .user-info {
            display: flex;
            align-items: center;
            gap: var(--space-3);
        }

        .user-avatar {
            width: 40px;
            height: 40px;
            border-radius: 50%;
            background: linear-gradient(135deg, var(--color-gold), #b8934a);
            display: flex;
            align-items: center;
            justify-content: center;
            font-weight: 600;
            color: var(--color-bg);
        }

        .user-details {
            flex: 1;
        }

        .user-name {
            font-weight: 600;
            font-size: 0.9rem;
        }

        .user-email {
            font-size: 0.8rem;
            color: var(--color-silver);
        }

        /* Main Content */
        .main-content {
            flex: 1;
            margin-left: var(--sidebar-width);
            padding: var(--space-6);
        }

        .header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: var(--space-6);
            flex-wrap: wrap;
            gap: var(--space-4);
        }

            .header h2 {
                font-size: 1.75rem;
                font-weight: 700;
            }

        .subtitle {
            color: var(--color-silver);
            margin-top: var(--space-1);
        }

        .btn {
            display: inline-flex;
            align-items: center;
            gap: var(--space-2);
            padding: var(--space-3) var(--space-4);
            border-radius: var(--radius-md);
            font-weight: 500;
            text-decoration: none;
            cursor: pointer;
            border: none;
            transition: all 0.2s ease;
        }

        .btn-primary {
            background: linear-gradient(135deg, var(--color-gold), #b8934a);
            color: var(--color-bg);
        }

            .btn-primary:hover {
                background: linear-gradient(135deg, var(--color-gold-hover), var(--color-gold));
                transform: translateY(-2px);
            }

        .btn-outline {
            background: transparent;
            border: 1px solid var(--color-border);
            color: var(--color-white);
        }

            .btn-outline:hover {
                background: var(--color-surface-2);
                border-color: var(--color-gold);
            }

        .btn-sm {
            padding: var(--space-2) var(--space-3);
            font-size: 0.85rem;
        }

        /* Stats Grid */
        .stats-grid {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: var(--space-5);
            margin-bottom: var(--space-6);
        }

        .stat-card {
            background: var(--color-surface);
            border: 1px solid var(--color-border);
            border-radius: var(--radius-lg);
            padding: var(--space-5);
            text-align: center;
        }

        .stat-icon {
            width: 50px;
            height: 50px;
            border-radius: var(--radius-md);
            display: flex;
            align-items: center;
            justify-content: center;
            margin: 0 auto var(--space-3);
            font-size: 1.25rem;
        }

        .stat-card.trips .stat-icon {
            background: rgba(212, 168, 83, 0.2);
            color: var(--color-gold);
        }

        .stat-card.upcoming .stat-icon {
            background: rgba(88, 166, 255, 0.2);
            color: var(--color-info);
        }

        .stat-card.wishlist .stat-icon {
            background: rgba(248, 81, 73, 0.2);
            color: var(--color-danger);
        }

        .stat-card.rewards .stat-icon {
            background: rgba(63, 185, 80, 0.2);
            color: var(--color-success);
        }

        .stat-value {
            font-size: 2rem;
            font-weight: 700;
            margin-bottom: var(--space-1);
        }

        .stat-label {
            color: var(--color-silver);
            font-size: 0.9rem;
        }

        /* Content Grid */
        .content-grid {
            display: grid;
            grid-template-columns: 1.5fr 1fr;
            gap: var(--space-5);
        }

        .card {
            background: var(--color-surface);
            border: 1px solid var(--color-border);
            border-radius: var(--radius-lg);
            overflow: hidden;
        }

        .card-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: var(--space-4) var(--space-5);
            border-bottom: 1px solid var(--color-border);
        }

            .card-header h3 {
                font-size: 1.1rem;
                font-weight: 600;
            }

        .card-body {
            padding: var(--space-5);
        }

        /* Booking Items */
        .booking-item {
            display: flex;
            gap: var(--space-4);
            padding: var(--space-4);
            border-bottom: 1px solid var(--color-border);
            transition: background 0.2s;
        }

            .booking-item:last-child {
                border-bottom: none;
            }

            .booking-item:hover {
                background: var(--color-surface-2);
            }

        .booking-image {
            width: 80px;
            height: 60px;
            object-fit: cover;
            border-radius: var(--radius-sm);
        }

        .booking-info {
            flex: 1;
        }

        .booking-destination {
            font-weight: 600;
            margin-bottom: var(--space-1);
        }

        .booking-dates {
            font-size: 0.85rem;
            color: var(--color-silver);
            margin-bottom: var(--space-2);
        }

        .booking-status {
            display: inline-flex;
            align-items: center;
            gap: 4px;
            padding: 4px 10px;
            border-radius: 20px;
            font-size: 0.75rem;
            font-weight: 500;
        }

            .booking-status.upcoming {
                background: rgba(88, 166, 255, 0.2);
                color: var(--color-info);
            }

            .booking-status.confirmed {
                background: rgba(63, 185, 80, 0.2);
                color: var(--color-success);
            }

            .booking-status.pending {
                background: rgba(210, 153, 34, 0.2);
                color: var(--color-warning);
            }

            .booking-status.cancelled {
                background: rgba(248, 81, 73, 0.2);
                color: var(--color-danger);
            }

        /* Package Cards */
        .package-card {
            border: 1px solid var(--color-border);
            border-radius: var(--radius-md);
            overflow: hidden;
            margin-bottom: var(--space-4);
        }

            .package-card:last-child {
                margin-bottom: 0;
            }

            .package-card img {
                width: 100%;
                height: 120px;
                object-fit: cover;
            }

        .package-card-body {
            padding: var(--space-4);
        }

        .package-location {
            font-size: 0.8rem;
            color: var(--color-gold);
            margin-bottom: var(--space-1);
        }

        .package-title {
            font-weight: 600;
            margin-bottom: var(--space-3);
        }

        .package-footer {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .package-price {
            font-size: 1.1rem;
            font-weight: 700;
            color: var(--color-gold);
        }

        /* Empty State */
        .empty-state {
            text-align: center;
            padding: var(--space-6);
            color: var(--color-silver);
        }

            .empty-state i {
                font-size: 3rem;
                margin-bottom: var(--space-4);
                opacity: 0.5;
            }

            .empty-state p {
                margin-bottom: var(--space-4);
            }

        /* Loading */
        .loading {
            display: flex;
            justify-content: center;
            padding: var(--space-6);
        }

        .spinner {
            width: 40px;
            height: 40px;
            border: 3px solid var(--color-border);
            border-top-color: var(--color-gold);
            border-radius: 50%;
            animation: spin 1s linear infinite;
        }

        @@keyframes spin {
            to {
                transform: rotate(360deg);
            }
        }

        /* Toast */
        .toast {
            position: fixed;
            bottom: 20px;
            right: 20px;
            background: var(--color-surface-2);
            border: 1px solid var(--color-border);
            padding: var(--space-4) var(--space-5);
            border-radius: var(--radius-md);
            display: flex;
            align-items: center;
            gap: var(--space-3);
            transform: translateY(100px);
            opacity: 0;
            transition: all 0.3s ease;
            z-index: 1000;
        }

            .toast.show {
                transform: translateY(0);
                opacity: 1;
            }

            .toast.success {
                border-color: var(--color-success);
            }

            .toast.error {
                border-color: var(--color-danger);
            }

        /* Mobile Menu */
        .mobile-menu-btn {
            display: none;
            background: none;
            border: none;
            color: var(--color-white);
            font-size: 1.5rem;
            cursor: pointer;
            margin-right: var(--space-3);
        }

        .sidebar-overlay {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: rgba(0,0,0,0.5);
            z-index: 99;
        }

        /* Responsive */
        @@media (max-width: 1024px) {
            .stats-grid {
                grid-template-columns: repeat(2, 1fr);
            }

            .content-grid {
                grid-template-columns: 1fr;
            }
        }

        @@media (max-width: 768px) {
            .sidebar {
                transform: translateX(-100%);
            }

                .sidebar.active {
                    transform: translateX(0);
                }

            .sidebar-overlay.active {
                display: block;
            }

            .main-content {
                margin-left: 0;
                padding: var(--space-4);
            }

            .mobile-menu-btn {
                display: block;
            }

            .header {
                flex-direction: column;
                align-items: flex-start;
            }

            .header-actions {
                width: 100%;
            }
        }

        @@media (max-width: 480px) {
            .stats-grid {
                grid-template-columns: 1fr 1fr;
                gap: var(--space-3);
            }

            .stat-card {
                padding: var(--space-4);
            }

            .stat-value {
                font-size: 1.5rem;
            }
        }
    </style>
</head>
<body>
    <div class="dashboard-layout">
        <!-- Sidebar -->
        <aside class="sidebar" id="sidebar">
            <div class="sidebar-header">
                <a href="/" class="sidebar-logo" onclick="navigateTo('/', event)">
                    <i class="fas fa-plane"></i> TRAVIL
                </a>
            </div>

            <nav class="sidebar-nav">
                <a href="/account/dashboard" class="nav-item active">
                    <i class="fas fa-th-large"></i>
                    <span>Dashboard</span>
                </a>
                <a href="/account/bookings" class="nav-item" onclick="navigateTo('/account/bookings', event)">
                    <i class="fas fa-calendar-check"></i>
                    <span>My Bookings</span>
                </a>
                <a href="/account/wishlist" class="nav-item" onclick="navigateTo('/account/wishlist', event)">
                    <i class="fas fa-heart"></i>
                    <span>Wishlist</span>
                </a>
                <a href="/packages" class="nav-item" onclick="navigateTo('/packages', event)">
                    <i class="fas fa-suitcase"></i>
                    <span>Browse Packages</span>
                </a>
                <a href="/account/profile" class="nav-item" onclick="navigateTo('/account/profile', event)">
                    <i class="fas fa-user"></i>
                    <span>Profile</span>
                </a>
                <a href="/account/settings" class="nav-item" onclick="navigateTo('/account/settings', event)">
                    <i class="fas fa-cog"></i>
                    <span>Settings</span>
                </a>
                <a href="#" class="nav-item" onclick="logout(event)">
                    <i class="fas fa-sign-out-alt"></i>
                    <span>Logout</span>
                </a>
            </nav>

            <div class="sidebar-footer">
                <div class="user-info">
                    <div class="user-avatar" id="userAvatar">U</div>
                    <div class="user-details">
                        <div class="user-name" id="userName">User</div>
                        <div class="user-email" id="userEmail">user@example.com</div>
                    </div>
                </div>
            </div>
        </aside>

        <div class="sidebar-overlay" id="sidebarOverlay"></div>

        <!-- Main Content -->
        <main class="main-content">
            <div class="header">
                <div>
                    <button class="mobile-menu-btn" id="mobileMenuBtn">
                        <i class="fas fa-bars"></i>
                    </button>
                    <h2>Welcome back, <span id="welcomeName">User</span>!</h2>
                    <p class="subtitle">Here's what's happening with your travel plans.</p>
                </div>
                <div class="header-actions">
                    <a href="/packages" class="btn btn-primary" onclick="navigateTo('/packages', event)">
                        <i class="fas fa-plus"></i> Book New Trip
                    </a>
                </div>
            </div>

            <!-- Stats Grid -->
            <div class="stats-grid">
                <div class="stat-card trips">
                    <div class="stat-icon"><i class="fas fa-plane"></i></div>
                    <div class="stat-value" id="totalTrips">0</div>
                    <div class="stat-label">Total Trips</div>
                </div>
                <div class="stat-card upcoming">
                    <div class="stat-icon"><i class="fas fa-calendar"></i></div>
                    <div class="stat-value" id="upcomingTrips">0</div>
                    <div class="stat-label">Upcoming</div>
                </div>
                <div class="stat-card wishlist">
                    <div class="stat-icon"><i class="fas fa-heart"></i></div>
                    <div class="stat-value" id="wishlistCount">0</div>
                    <div class="stat-label">Wishlist</div>
                </div>
                <div class="stat-card rewards">
                    <div class="stat-icon"><i class="fas fa-award"></i></div>
                    <div class="stat-value" id="rewardPoints">0</div>
                    <div class="stat-label">Reward Points</div>
                </div>
            </div>

            <div class="content-grid">
                <!-- Recent Bookings -->
                <div class="card">
                    <div class="card-header">
                        <h3>Recent Bookings</h3>
                        <a href="/account/bookings" class="btn btn-outline btn-sm" onclick="navigateTo('/account/bookings', event)">View All</a>
                    </div>
                    <div class="card-body" id="bookingsContainer">
                        <div class="loading" id="bookingsLoader">
                            <div class="spinner"></div>
                        </div>
                        <div class="empty-state" id="noBookings" style="display: none;">
                            <i class="fas fa-calendar-times"></i>
                            <p>You haven't made any bookings yet.</p>
                            <a href="/packages" class="btn btn-primary" onclick="navigateTo('/packages', event)">
                                <i class="fas fa-search"></i> Browse Packages
                            </a>
                        </div>
                        <div id="bookingsList"></div>
                    </div>
                </div>

                <!-- Recommended Packages -->
                <div class="card">
                    <div class="card-header">
                        <h3>Recommended For You</h3>
                    </div>
                    <div class="card-body" id="recommendedContainer">
                        <div class="loading" id="recommendedLoader">
                            <div class="spinner"></div>
                        </div>
                        <div class="empty-state" id="noPackages" style="display: none;">
                            <i class="fas fa-suitcase"></i>
                            <p>No packages available yet.</p>
                        </div>
                        <div id="recommendedList"></div>
                    </div>
                </div>
            </div>
        </main>
    </div>

    <div id="toast" class="toast">
        <i class="fas fa-check-circle"></i>
        <span id="toastMessage">Message</span>
    </div>

    <script>
        // Navigation
        function navigateTo(url, event) {
            if (event) event.preventDefault();
            window.location.href = url;
        }

        // Logout
        function logout(event) {
            if (event) event.preventDefault();
            localStorage.removeItem('token');
            localStorage.removeItem('user');
            navigateTo('/account/login');
        }

        // Toast
        function showToast(message, type = 'success') {
            const toast = document.getElementById('toast');
            const toastMessage = document.getElementById('toastMessage');
            toastMessage.textContent = message;
            toast.className = `toast ${type} show`;
            setTimeout(() => toast.classList.remove('show'), 3000);
        }

        // Format date
        function formatDate(dateString) {
            const date = new Date(dateString);
            return date.toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' });
        }

        // Get status class
        function getStatusClass(status) {
            const statusLower = (status || '').toLowerCase();
            if (statusLower === 'confirmed') return 'confirmed';
            if (statusLower === 'pending') return 'pending';
            if (statusLower === 'cancelled') return 'cancelled';
            return 'upcoming';
        }

        // Get status icon
        function getStatusIcon(status) {
            const statusLower = (status || '').toLowerCase();
            if (statusLower === 'confirmed') return 'fa-check';
            if (statusLower === 'pending') return 'fa-hourglass-half';
            if (statusLower === 'cancelled') return 'fa-times';
            return 'fa-clock';
        }

        // Mobile menu
        document.getElementById('mobileMenuBtn').addEventListener('click', function() {
            document.getElementById('sidebar').classList.toggle('active');
            document.getElementById('sidebarOverlay').classList.toggle('active');
        });

        document.getElementById('sidebarOverlay').addEventListener('click', function() {
            document.getElementById('sidebar').classList.remove('active');
            this.classList.remove('active');
        });

        // Initialize
        document.addEventListener('DOMContentLoaded', async function() {
            const token = localStorage.getItem('token');
            const user = localStorage.getItem('user');

            if (!token || !user) {
                navigateTo('/account/login');
                return;
            }

            try {
                const userData = JSON.parse(user);

                // Set user info
                const firstName = userData.firstName || 'User';
                const lastName = userData.lastName || '';
                document.getElementById('welcomeName').textContent = firstName;
                document.getElementById('userName').textContent = `${firstName} ${lastName}`.trim();
                document.getElementById('userEmail').textContent = userData.email || '';
                document.getElementById('userAvatar').textContent = firstName.charAt(0).toUpperCase();

                // Load data
                await loadUserBookings();
                await loadRecommendedPackages();

            } catch (error) {
                console.error('Error:', error);
                showToast('Error loading dashboard', 'error');
            }
        });

        // Load user bookings
        async function loadUserBookings() {
            const token = localStorage.getItem('token');
            const bookingsLoader = document.getElementById('bookingsLoader');
            const noBookings = document.getElementById('noBookings');
            const bookingsList = document.getElementById('bookingsList');

            try {
                const response = await fetch('/api/booking/my-bookings', {
                    headers: {
                        'Authorization': `Bearer ${token}`,
                        'Content-Type': 'application/json'
                    }
                });

                bookingsLoader.style.display = 'none';

                if (response.ok) {
                    const result = await response.json();
                    const bookings = result.data || [];

                    console.log('Bookings API response:', result);
                    console.log('Bookings data:', bookings);

                    // Update stats
                    document.getElementById('totalTrips').textContent = bookings.length;

                    const now = new Date();
                    const upcoming = bookings.filter(b => {
                        const startDate = new Date(b.startDate || b.travelPackage?.startDate);
                        return startDate > now && b.status !== 'Cancelled';
                    });
                    document.getElementById('upcomingTrips').textContent = upcoming.length;

                    if (bookings.length === 0) {
                        noBookings.style.display = 'block';
                    } else {
                        // Show last 3 bookings
                        const recentBookings = bookings.slice(0, 3);
                        bookingsList.innerHTML = recentBookings.map(booking => {
                            // API returns FLAT structure - data is directly on booking object
                            // Support both flat (API) and nested (travelPackage) formats
                            const pkg = booking.travelPackage || booking;
                            const imageUrl = booking.imageUrl || pkg.imageUrl || 'https://images.unsplash.com/photo-1507525428034-b723cf961d3e?w=200&q=80';
                            const destination = booking.destination || pkg.destination || 'Unknown Destination';
                            const startDate = formatDate(booking.startDate || pkg.startDate);
                            const endDate = formatDate(booking.endDate || pkg.endDate);
                            const status = booking.status || 'Pending';

                            return `
                                <div class="booking-item" onclick="window.location.href='/my-trips'" style="cursor: pointer;">
                                    <img src="${imageUrl}" alt="${destination}" class="booking-image" onerror="this.src='https://images.unsplash.com/photo-1507525428034-b723cf961d3e?w=200&q=80'">
                                    <div class="booking-info">
                                        <div class="booking-destination">${destination}</div>
                                        <div class="booking-dates"><i class="far fa-calendar"></i> ${startDate} - ${endDate}</div>
                                        <span class="booking-status ${getStatusClass(status)}">
                                            <i class="fas ${getStatusIcon(status)}"></i> ${status}
                                        </span>
                                    </div>
                                </div>
                            `;
                        }).join('');
                    }
                } else {
                    console.error('Failed to load bookings. Status:', response.status);
                    const errorText = await response.text();
                    console.error('Error response:', errorText);
                    noBookings.style.display = 'block';
                    document.getElementById('totalTrips').textContent = '0';
                    document.getElementById('upcomingTrips').textContent = '0';
                }
            } catch (error) {
                console.error('Error loading bookings:', error);
                bookingsLoader.style.display = 'none';
                noBookings.style.display = 'block';
            }
        }

        // Load recommended packages
        async function loadRecommendedPackages() {
            const recommendedLoader = document.getElementById('recommendedLoader');
            const noPackages = document.getElementById('noPackages');
            const recommendedList = document.getElementById('recommendedList');

            try {
                const response = await fetch('/api/packages/popular?count=2');

                recommendedLoader.style.display = 'none';

                if (response.ok) {
                    const result = await response.json();
                    const packages = result.data || [];

                    if (packages.length === 0) {
                        noPackages.style.display = 'block';
                    } else {
                        recommendedList.innerHTML = packages.map(pkg => {
                            const imageUrl = pkg.imageUrl || 'https://images.unsplash.com/photo-1507525428034-b723cf961d3e?w=400&q=80';
                            const price = pkg.discountedPrice || pkg.price || 0;

                            return `
                                <div class="package-card">
                                    <img src="${imageUrl}" alt="${pkg.destination}" onerror="this.src='https://images.unsplash.com/photo-1507525428034-b723cf961d3e?w=400&q=80'">
                                    <div class="package-card-body">
                                        <div class="package-location"><i class="fas fa-map-marker-alt"></i> ${pkg.country || ''}</div>
                                        <div class="package-title">${pkg.destination || 'Package'}</div>
                                        <div class="package-footer">
                                            <span class="package-price">$${price.toLocaleString()}</span>
                                            <a href="/packages/${pkg.packageId}" class="btn btn-outline btn-sm" onclick="navigateTo('/packages/${pkg.packageId}', event)">View</a>
                                        </div>
                                    </div>
                                </div>
                            `;
                        }).join('');
                    }
                } else {
                    noPackages.style.display = 'block';
                }
            } catch (error) {
                console.error('Error loading packages:', error);
                recommendedLoader.style.display = 'none';
                noPackages.style.display = 'block';
            }
        }
    </script>
</body>
</html>
